    <meta name="twitter:domain" content="https://powerofpower.net">

    <meta property="fb:app_id" content="1973805279585727">
    <meta property="og:image" content="{{ comic.image }}">
    <meta property="og:title" content="Power of Power - {{ comic.title }}">
    <meta property="og:url" content="https://powerofpower.net/comics/{{ comic.index }}">
    <meta property="og:site_name" content="Power of Power">
