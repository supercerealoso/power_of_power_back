    <meta charset="utf-8">
    <meta name="robots" content="index, follow">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta name="msvalidate.01" content="64791B2843C37E5B4A08D38FB20261E1">
    <meta name="google-site-verification" content="QIcSwGa6HiIdNgkGgM-9KC9f6srJtiAQPeN8aIjV2Hw">

    <meta name="keywords" content="supercerealoso, powerofpower, power of power, webcomic, {{ comic.keywords }}">
    <meta name="description"
        content="{{ comic.title }} - There is not enough statistical evidence to reject that this comic is cool. {{ comic.keywords }}">

    <meta name="twitter:card" content="photo">
    <meta name="twitter:site" content="@pwrofpwr">
    <meta name="twitter:creator" content="@pwrofpwr">
    <meta name="twitter:title" content="Power of Power - {{ comic.title }}">
