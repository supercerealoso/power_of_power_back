        content="There is not enough statistical evidence to reject that this comic is cool.">

    <style>
        @font-face {
            font-family: 'supercerealoso';
            src: url('/supercerealoso.woff2') format('woff2'), url('/supercerealoso.woff') format('woff');
            font-weight: normal;
            font-style: normal;
        }

        .navbar-dark {
            background-color: #074c97 !important;
        }

        .navbar-brand {
            font-family: 'supercerealoso';
        }
    </style>
</head>

<body itemscope itemtype="http://schema.org/ComicStory">
    <h1 class="d-none">Power of Power - {{ comic.title }} - {{ comic.keywords }}</h1>
    <nav class="navbar navbar-dark bg-dark">
        <div class="container text-center">
            <a class="navbar-brand" href="/">Power of Power</a>
        </div>
    </nav>
    <div class="container text-center">
        @if(comic.special)
        <div>{{ raw(comic.special) }}</div>
        @else
        <p><a href="/"><img itemprop="image" class="img-fluid" src="{{ comic.image }}" alt="{{ comic.alt }}"
                    title="{{ comic.alt }}"></a></p>
        @endif
        <div class="card text-left">
            <div class="card-body">
                <h5 class="card-title" itemprop="name">{{ comic.title }}</h5>
                <h6 class="card-subtitle mb-2 text-muted">supercerealoso:</h6>
                <div class="card-text">{{ raw(comic.comment) }}</div>
                <small class="card-text">Posted: {{ comic.posted.toDateString() }}</small>
                <small class="card-text">Keywords: {{ comic.keywords }}</small>
                <small class="card-text" itemprop="description">Transcript: {{ raw(comic.transcript) }}</small>
            </div>
        </div>
        <img src="{{ comic.thumb }}" alt="thumb">
        <a class="nav-link" href="https://powerofpower.herokuapp.com/">Admin</a>
    </div>
    <!--
