    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-37190779-1"></script>
    <script>
        // google analytics
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'UA-37190779-1');
        // render last comic link
        var last = +'{{ comic.index }}';
        function random() {
            var index = Math.floor(Math.random() * last) + 1;
            window.location.href = '/comics/' + index;
        }
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState === 4 && this.status === 200) {
                var count = JSON.parse(this.responseText).count;
                if (last < count) {
                    document.getElementById('next').href = '/comics/' + (last + 1);
                    document.getElementById('last').href = '/comics/' + count;
                    last = count;
                }
            }
        };
        xhttp.open('GET', '/comics/count.json', true);
        xhttp.send();
    </script>
